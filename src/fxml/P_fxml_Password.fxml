<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>

<HBox alignment="CENTER_LEFT" spacing="5.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.P_FXML_Password_Controller">
   <children>
      <TextField fx:id="passTextField" minWidth="300.0" onAction="#passTextFieldEnterPressed" promptText="SQL Workspace Password" />
      <Button fx:id="connectButton" mnemonicParsing="false" onAction="#connectButtonClicked" text="Connect" />
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</HBox>
